{
  "name": "show-episode-format",
  "version": "2.3.0",
  "description": "Format your episodes to 01x01 or S01E01 strings and group by air date",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel -d dist/ src/",
    "build:watch": "babel --watch -d dist/ src/",
    "lint": "eslint 'src/**/*.js'",
    "prebuild": "rm -rf dist && mkdir dist",
    "precommit": "npm run lint && npm test",
    "prepublish": "npm run build",
    "start": "node cli.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ewnd9/show-episode-format.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "show",
    "format"
  ],
  "author": "ewnd9 <ewndnine@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "lodash.groupby": "^4.6.0",
    "pluralize": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^3.0.0",
    "husky": "^0.11.6",
    "timekeeper": "^1.0.0"
  }
}
